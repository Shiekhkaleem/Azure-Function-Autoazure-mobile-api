{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "ConnectionStrings": {
    "AutoAzure-DEV": "###",
    "AutoAzure-PROD": "###",
    "AutoAzure-NOTI": "###"
  },
  "JwtToken": {
    "Key": "###"
  },
  "App-Keys": {
    "Key": "###",
    "Prod-Key": "###"
  },
  "SMTPConfig": {
    "Host": "smtp.mailgun.org",
    "Port": "587",
    "MailTo": "",
    "ReplyTo": "###",
    "ReplyToName": "AutoAzure",
    "ReplyToHelp": "###",
    "ReplyToHelpName": "Support BHW",
    "MailFrom": "###",
    "MailFromName": "AutoAzure",
    "SMTPUser": "apikey",
    "SMTPPass": "###"
  },
