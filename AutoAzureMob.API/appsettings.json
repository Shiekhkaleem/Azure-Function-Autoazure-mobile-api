      "applicationUrl": "http://localhost:7124;https://autoazur-mob-api-dev.azurewebsites.net",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "AutoAzureMob-Prod": {
      "commandName": "AutoAzureMob-Prod",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:7124;https://prod-mtf-api.azurewebsites.net",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
